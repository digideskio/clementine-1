<h1>Projects</h1>

<div class="row">
  <% @projects.each do |project| %>
    <div class="col-md-6">
      <%= link_to admin_project_path(project) do %>
        <%= render 'project_card', project: project %>
      <% end %>
    </div>
  <% end %>
</div>


<br>

<%= link_to 'New Project', new_admin_project_path %>
